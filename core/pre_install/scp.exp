#!/bin/expect

set timeout 60
set node [lindex $argv 0]

spawn scp -r ../../.. $node:/var/ysm/

expect {
    ".ssh/id_rsa\)" { send "\r"; exp_continue }
    "Overwrite (y/n)?" { send "y\r"; exp_continue }
    "empty for no passphrase\)" { send "\r"; exp_continue }
    "passphrase again" { send "\r"; exp_continue }
}